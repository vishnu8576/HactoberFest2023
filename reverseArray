#include<bits/stdc++.h>
using namespace std;

void revers(int arr[],int n)
{
    int low=0, high=n-1;
    while(low<high)
    {
        int temp=arr[low];
        arr[low]=arr[high];
        arr[high]=temp;
        low++;
        high--;
    }
}

int main()
{
    int n=5;
    int arr[n]={10,13,17,19,32};

    revers(arr,n);
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<"  ";
    }

    return 0;
}
